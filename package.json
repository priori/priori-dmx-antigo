{
  "name": "priori-dmx",
  "productName": "priori-dmx",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/main.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint --project tsconfig.json --type-check --force",
    "watchpage": "./node_modules/.bin/tsc -p ./tsconfig-page.json",
    "prettier": "./node_modules/.bin/prettier --write \"./src/**/*.{ts,tsx}\"",
    "parcel": "parcel watch ./src/web.html --cache-dir ../priori-dmx-yarn-cache --out-dir ./public",
    "parcel-build": "parcel build ./src/web.html --cache-dir ../priori-dmx-yarn-cache --out-dir ./public"
  },
  "keywords": [],
  "author": "Leonardo",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "priori_dmx"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "prioridmx"
      }
    }
  },
  "dependencies": {
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.20",
    "body-parser": "^1.18.3",
    "default-gateway": "^5.0.4",
    "dmx": "0.0.7",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.3",
    "express-ws": "^4.0.0",
    "parcel": "^1.10.3",
    "prettier": "^1.14.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-slider": "^0.11.2",
    "react-sortable-hoc": "^0.8.3",
    "tslib": "^1.4.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "3.0.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "tslint": "^5.1.0",
    "typescript": "~2.2.2"
  }
}
